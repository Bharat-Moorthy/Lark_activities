    "## 3. Write a program to find the area and perimeter of a rectangle using functions"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Enter the length in cm of the rectangle: 2\n",
      "Enter the breadth in cm of the rectangle: 3\n",
      "Area of the rectangle is  6.0 cm2\n",
      "perimeter of the rectangle is  10.0 cm\n"
     ]
    }
   ],
   "source": [
    "#area and perimeter of rectangele\n",
    "def rectangle(l,b):\n",
    "    area =l*b\n",
    "    perimeter =2*(l+b)\n",
    "    print(\"Area of the rectangle is \", area ,\"cm2\")\n",
    "    print(\"perimeter of the rectangle is \", perimeter , \"cm\")\n",
    "\n",
    "l=float(input(\"Enter the length in cm of the rectangle: \"))\n",
    "b =float(input(\"Enter the breadth in cm of the rectangle: \"))\n",
    "rectangle(l,b)"
   ]
  },
