    "## 2. Write a program to count the number of each characters in a string"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Enter a string:Bharat\n",
      "Count of each characters is  {'B': 1, 'h': 1, 'a': 2, 'r': 1, 't': 1}\n"
     ]
    }
   ],
   "source": [
    "lst={}\n",
    "string=str(input(\"Enter a string:\"))\n",
    "for i in string:\n",
    "    if i in lst:\n",
    "        lst[i]+=1\n",
    "    else :\n",
    "        lst[i]=1\n",
    "print(\"Count of each characters is \" , (lst))"
   ]
  },
