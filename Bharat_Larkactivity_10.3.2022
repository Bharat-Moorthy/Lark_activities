  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Enter Number of Rows: \n",
      "5\n",
      "The pyramid with 5 number of rows is \n",
      "     * \n",
      "    * * \n",
      "   * * * \n",
      "  * * * * \n",
      " * * * * * \n"
     ]
    }
   ],
   "source": [
    "print(\"Enter Number of Rows: \")\n",
    "row= int(input())\n",
    "print(\"The pyramid with\", row , \"number of rows is \")\n",
    "#print(\"Star Pyramid of \" + str(row) + \" Rows or Lines: \")\n",
    "for i in range(row):\n",
    "    for j in range(row, i, -1):\n",
    "        print(end=\" \")\n",
    "    for k in range(i+1):\n",
    "        print(end=\"* \")\n",
    "    print()"
